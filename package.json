{
  "name": "apollo-graphql-server-starter-with-prisma-and-typescript",
  "version": "1.0.0",
  "description": "Boilerplate forl Apollo GraphQL Server w/ Prisma and TypeScript",
  "main": "src/app.ts",
  "repository": "git@github.com:thekalko/apollo-graphql-server-starter-with-prisma-and-typescript.git",
  "author": "thekalko <thecyberkalko@gmail.hu>",
  "license": "GPL-v3",
  "private": true,
  "scripts": {
    "dev": "yarn build && nodemon dist/src/app.js",
    "prod": "yarn build && node dist/src/app.js",
    "build": "rm -rf dist && tsc -p . && yarn prisma-generate",
    "prisma": "npx prisma",
    "prisma-generate": "npx prisma generate",
    "prisma-migrate": "npx prisma migrate save --experimental && npx prisma migrate up --experimental",
    "migrate": "yarn prisma-migrate && yarn prisma-generate",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "@prisma/client": "^2.12.1",
    "@types/express": "^4.17.2",
    "apollo-server-express": "^2.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-import": "^1.0.0-beta.2",
    "graphql-tools": "^7.0.2",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.2",
    "ts-invariant": "^0.6.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@prisma/cli": "^2.12.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "ts-node": "^9.1.0"
  },
  "_moduleAliases": {
    "prisma": "dist/prisma/prisma.js",
    "src": "dist/src"
  }
}
