{
  "name": "apollo-graphql-server-starter-with-prisma-and-typescript",
  "version": "1.0.0",
  "description": "Boilerplate forl Apollo GraphQL Server w/ Prisma and TypeScript",
  "main": "src/app.ts",
  "repository": "git@github.com:thekalko/apollo-graphql-server-starter-with-prisma-and-typescript.git",
  "author": "thekalko <thecyberkalko@gmail.hu>",
  "license": "GPL-v3",
  "private": true,
  "scripts": {
    "dev": "yarn build:dev && nodemon dist/src/app.js",
    "prod": "yarn build:production && node build/src/app.js",
    "build:dev": "rm -rf dist && tsc -p .",
    "prisma": "prisma",
    "build:production": "rm -rf build && tsc -p tsconfig.release.json",
    "prisma-deploy": "prisma export && prisma deploy && prisma generate",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "apollo-server-express": "^2.10.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.0-beta.2",
    "graphql-tools": "^4.0.7",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.2",
    "prisma-client-lib": "^1.34.10",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "ts-node": "^8.6.2"
  },
  "_moduleAliases": {
    "prisma": "dist/database/generated/index.js"
  }
}
